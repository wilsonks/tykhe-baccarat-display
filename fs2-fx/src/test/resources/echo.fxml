<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" prefHeight="400" prefWidth="600"
      xmlns="http://javafx.com/javafx/8.0.121" fx:controller="fs2.io.fx.EchoWindow">
    <TextField fx:id="message" prefWidth="200.0"/>
    <HBox>
        <Button fx:id="change" mnemonicParsing="false" text="change"/>
        <TextField fx:id="input" prefWidth="200.0"/>
    </HBox>
    <Pane fx:id="pane" prefHeight="200" prefWidth="300.0"/>
    <HBox>
        <Button fx:id="restart" mnemonicParsing="false" text="RESTART"/>
    </HBox>
</VBox>
